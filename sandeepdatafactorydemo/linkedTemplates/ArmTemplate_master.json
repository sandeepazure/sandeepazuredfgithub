{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sandeepdatafactorydemo"
		},
		"AzureBlobStorage2020_10_08_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2020_10_08'"
		},
		"az_destinationdbconnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'az_destinationdbconnection'"
		},
		"az_dfblob_20201007_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'az_dfblob_20201007'"
		},
		"az_sourceblobconnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'az_sourceblobconnection'"
		},
		"az_sqldb_20201007_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'az_sqldb_20201007'"
		},
		"azuredestinationdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azuredestinationdb'"
		},
		"moviedbls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'moviedbls'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sandeepdatafactorydemo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage2020_10_08_connectionString": {
						"value": "[parameters('AzureBlobStorage2020_10_08_connectionString')]"
					},
					"az_destinationdbconnection_connectionString": {
						"value": "[parameters('az_destinationdbconnection_connectionString')]"
					},
					"az_dfblob_20201007_connectionString": {
						"value": "[parameters('az_dfblob_20201007_connectionString')]"
					},
					"az_sourceblobconnection_connectionString": {
						"value": "[parameters('az_sourceblobconnection_connectionString')]"
					},
					"az_sqldb_20201007_connectionString": {
						"value": "[parameters('az_sqldb_20201007_connectionString')]"
					},
					"azuredestinationdb_connectionString": {
						"value": "[parameters('azuredestinationdb_connectionString')]"
					},
					"moviedbls_connectionString": {
						"value": "[parameters('moviedbls_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "sandeepdatafactorydemo_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sandeepdatafactorydemo_ArmTemplate_0')]"
			]
		}
	]
}